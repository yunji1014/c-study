#include <iostream>
using namespace std;

int main() {
    int a;
    cin >> a;

    if(89 < a){
        std::cout << "A" << std::endl;
    }
    else if(79 < a){
        std::cout << "B" << std::endl;
    }
    else if(69 < a){
        std::cout << "C" << std::endl;
    }
    else if(59 < a){
        std::cout << "D" << std::endl;
    }else{
        std::cout << "F" << std::endl;
    }
}

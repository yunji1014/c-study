#include <iostream>
using namespace std;

int main() {
    string a, c;
    float b, d, sum, SUM;

    for(int i = 0; i < 20; i++){
        cin >> a >> b >> c;
        
        if(c=="A+"){
            d = 4.5;
        }else if(c=="A0"){
            d = 4.0;
        }else if(c=="B+"){
            d = 3.5;
        }else if(c=="B0"){
            d = 3.0;
        }else if(c=="C+"){
            d = 2.5;
        }else if(c=="C0"){
            d = 2.0;
        }else if(c=="D+"){
            d = 1.5;
        }else if(c=="D0"){
            d = 1.0;
        }else if(c=="F"){
            d = 0.0;
        }else{
            d = 0.0;
            b = 0.0;
        }
        sum += d*b;
        SUM += b;
    }
    
    cout << sum/SUM;
}

#include <iostream>
using namespace std;

int main() {
    int arr[101];
    int a, c;
    int b = 0;
    scanf("%d", &a);

    for(int i = 0; i<a; i++){
        scanf("%d", &arr[i]);
    }
    scanf("%d", &c);

    if(c >= -100 && c <= 100){
        for(int i = 0; i<a; i++){
            if(arr[i] == c){
                b++;
            }
        }
    }
    printf("%d", b);
    return 0;
}

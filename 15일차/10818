#include <iostream>
using namespace std;

int main() {
    int arr[1000000];
    int a;
    int Min = 1000000;
    int Max = -1000000;
    scanf("%d", &a);

    for(int i = 0; i<a; i++){
        scanf("%d", &arr[i]);
    }

    for(int i = 0; i<a; i++){
        if(arr[i] < Min){
                Min = arr[i];
            }
        if(arr[i] > Max){
            Max = arr[i];
        }
    }
    printf("%d %d", Min, Max);
} 

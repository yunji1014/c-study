#include <iostream>
using namespace std;

int main() {
    int arr[1000000];
    int b;
    int Max = -1000000;

    for(int i = 0; i<9; i++){
        scanf("%d", &arr[i]);
    }

    for(int i = 0; i<9; i++){
        if(arr[i] > Max){
            Max = arr[i];
            b = i+1;
        }
    }
    printf("%d %d", Max, b);
}

#include <iostream>
using namespace std;

int main() {
    int a,b;
    cin >> a;
    int count = 0;
    int c = 1;
    for(int i=0; i<a; i++){
        cin >> b;
        if(b==1){
            count = c + count;
            c++;
        }else if(b==0){
            c = 1;
        }
    }
    cout << count;
    return 0;
}

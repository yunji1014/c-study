#include <iostream>
using namespace std;

int main()
{
    char arr[1000];
    int a;

    scanf("%d", &a);
    for(int i = 0; i<a; i++){
        scanf("%s", arr);
        cout << arr[0];
        for(int j = 0; j < 1000; j++){
            if (arr[j] == 0){
                cout << arr[j-1] << "\n";
                break;
            }
        }
    }
}

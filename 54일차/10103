#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    int B = 100;
    int C = 100;
    cin >> a;

    for(int i=0; i<a; i++){
        cin >> b >> c;
        if(b<c){
            B = B - c;
        }else if(b>c){
            C = C - b;
        }
    }

    cout << B << "\n" << C;
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;

    if(0 < a && 0 < b){
        std::cout << "1" << std::endl;
    }
    else if(a < 0 && 0 < b){
        std::cout << "2" << std::endl;
    }
    else if(a < 0 && b < 0){
        std::cout << "3" << std::endl;
    }else{
        std::cout << "4" << std::endl;
    }
}

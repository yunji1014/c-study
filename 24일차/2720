#include <iostream>
using namespace std;

int main() {
    int a, b;
    int A,B,C,D;

    cin >> a;

    for(int i=0; i<a; i++){
        cin >> b;

        A = b/25;
        B = (b%25)/10;
        C = ((b%25)%10)/5;
        D = ((b%25)%10)%5;

        cout << A << " "<< B << " " << C << " " << D << "\n";
    }

    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;
    int A = a+b+c;
    
    if(A==180){
        if(a==b&&b==c){
            cout << "Equilateral";
        }else if(a==b||b==c||a==c){
            cout << "Isosceles";
        }else{
            cout << "Scalene";
        }
    }else{
        cout << "Error";
    }
} 
